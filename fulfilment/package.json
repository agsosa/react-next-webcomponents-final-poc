{
  "name": "fulfilment",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:wc\" --names \"NEXT,WC\" --prefix-colors \"blue,green\"",
    "dev:next": "next dev --turbopack -p 3002",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:wc": "vite build --config vite.wc.config.ts",
    "dev:wc": "vite build --config vite.wc.config.ts --watch"
  },
  "dependencies": {
    "@r2wc/react-to-web-component": "^2.0.4",
    "@types/styled-components": "^5.1.34",
    "next": "15.4.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.2",
    "typescript": "^5",
    "vite": "^7.0.5"
  }
}
